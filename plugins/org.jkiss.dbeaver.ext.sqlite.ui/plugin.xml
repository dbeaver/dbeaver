<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>

<plugin>

    <extension point="org.jkiss.dbeaver.dataManager">
        <manager class="org.jkiss.dbeaver.ext.sqlite.ui.data.manager.SQLiteValueManager" id="org.jkiss.dbeaver.ext.sqlite.ui.data.manager.SQLiteValueManager">
            <supports dataSource="org.jkiss.dbeaver.ext.sqlite.model.SQLiteDataSource"/>
        </manager>
    </extension>

    <extension point="org.jkiss.dbeaver.dataSourceView">
        <view
                class="org.jkiss.dbeaver.ext.sqlite.ui.views.SQLiteConnectionPage"
                id="com.dbeaver.db.sqlite.dataSourceWizardView"
                dataSource="sqlite"
                targetID="org.jkiss.dbeaver.ext.ui.newConnectionWizard">
        </view>
        <view
                class="org.jkiss.dbeaver.ext.sqlite.ui.views.SQLiteConnectionPage"
                id="com.dbeaver.db.sqlite.dataSourceEditorView"
                dataSource="sqlite"
                targetID="org.jkiss.dbeaver.ext.ui.editConnectionDialog">
        </view>
    </extension>

</plugin>
