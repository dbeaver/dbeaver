                id="org.jkiss.dbeaver.ext.bigquery.dataSourceEditorView"
                dataSource="bigquery"
                label="%bigquery.dialog.connection.header"
                targetID="org.jkiss.dbeaver.ext.ui.editConnectionDialog">
        </view>
    </extension>

</plugin>
