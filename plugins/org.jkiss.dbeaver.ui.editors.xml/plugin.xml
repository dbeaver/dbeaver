<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>

<plugin>

    <extension point="org.jkiss.dbeaver.dataManager">
        <streamManager class="org.jkiss.dbeaver.ui.data.managers.stream.XMLStreamValueManager" id="xml" commandId="org.jkiss.dbeaver.core.stream.xml" primaryMime="text/xml"/>
    </extension>

    <extension point="org.eclipse.ui.commands">
        <command id="org.jkiss.dbeaver.core.stream.xml" name="XML" description="XML" categoryId="org.jkiss.dbeaver.core.sql"/>
    </extension>

    <extension point="org.eclipse.ui.commandImages">
        <image commandId="org.jkiss.dbeaver.core.stream.xml" icon="platform:/plugin/org.jkiss.dbeaver.model/icons/types/xml.png"/>
    </extension>

    <extension point="org.eclipse.ui.bindings">
        <key commandId="org.jkiss.dbeaver.core.stream.xml" contextId="org.jkiss.dbeaver.ui.context.resultset" schemeId="org.eclipse.ui.defaultAcceleratorConfiguration" sequence="CTRL+SHIFT+X"/>
    </extension>

    <extension point="org.eclipse.ui.editors">
        <editor
              class="org.jkiss.dbeaver.ui.editors.xml.XMLEditor"
              default="true"
              extensions="xml"
              icon="platform:/plugin/org.jkiss.dbeaver.model/icons/types/xml.png"
              id="org.jkiss.dbeaver.ui.editors.xml.XMLEditor"
              name="XML">
            <contentTypeBinding contentTypeId="org.eclipse.core.runtime.xml"/>
        </editor>
    </extension>

</plugin>
