      <xsd:enumeration value="Merge Interval" />
      <xsd:enumeration value="Merge Join" />
      <xsd:enumeration value="Nested Loops" />
      <xsd:enumeration value="Online Index Insert" />
      <xsd:enumeration value="Parallelism" />
