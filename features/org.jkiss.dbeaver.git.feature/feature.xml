<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jkiss.dbeaver.git.feature"
      label="%featureName"
      version="1.0.128.qualifier"
      provider-name="%providerName">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.jkiss.dbeaver.ui.feature" version="7.0.0" match="greaterOrEqual"/>

      <import feature="org.eclipse.jgit" version="0.0.0" match="equivalent"/>
      <import feature="org.eclipse.jgit.gpg.bc" version="0.0.0" match="equivalent"/>
      <import feature="org.eclipse.jgit.http.apache" version="0.0.0" match="equivalent"/>
      <import feature="org.eclipse.jgit.ssh.apache" version="0.0.0" match="equivalent"/>

      <import plugin="org.eclipse.team.core" version="0.0.0" match="compatible"/>
   </requires>

   <!-- EGit deps -->
   <plugin id="org.eclipse.egit.core" version="0.0.0"/>
   <plugin id="org.eclipse.egit.ui" version="0.0.0"/>
   <plugin id="org.eclipse.egit" version="0.0.0"/>

   <!-- DBeaver deps -->
   <plugin id="org.jkiss.dbeaver.team.git" version="0.0.0"/>
   <plugin id="org.jkiss.dbeaver.team.git.ui" version="0.0.0"/>

</feature>
